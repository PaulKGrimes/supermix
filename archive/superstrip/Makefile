####### Implicit rules

.SUFFIXES: .c .o .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS) $(OBJCOMP)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: SuperStrip2.pro  /usr/qt/3/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile SuperStrip2.pro
qmake: 
	@$(QMAKE) -o Makefile SuperStrip2.pro

dist: 
	@mkdir -p .obj/SuperStrip2 && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/SuperStrip2/ && $(COPY_FILE) --parents superstrip2form.ui.h .obj/SuperStrip2/ && ( cd `dirname .obj/SuperStrip2` && $(TAR) SuperStrip2.tar SuperStrip2 && $(GZIP) SuperStrip2.tar ) && $(MOVE) `dirname .obj/SuperStrip2`/SuperStrip2.tar.gz . && $(DEL_FILE) -r .obj/SuperStrip2

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/main.o: main.cpp .ui/superstrip2form.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.ui/superstrip2form.h: superstrip2form.ui 
	$(UIC) superstrip2form.ui -o .ui/superstrip2form.h

.ui/superstrip2form.cpp: .ui/superstrip2form.h superstrip2form.ui 
	$(UIC) superstrip2form.ui -i superstrip2form.h -o .ui/superstrip2form.cpp

.obj/superstrip2form.o: .ui/superstrip2form.cpp superstrip2form.ui.h \
		.ui/superstrip2form.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/superstrip2form.o .ui/superstrip2form.cpp

.obj/moc_superstrip2form.o: .moc/moc_superstrip2form.cpp .ui/superstrip2form.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_superstrip2form.o .moc/moc_superstrip2form.cpp

.moc/moc_superstrip2form.cpp: $(MOC) .ui/superstrip2form.h
	$(MOC) .ui/superstrip2form.h -o .moc/moc_superstrip2form.cpp

