set size 0.9,1.0

set key left
set xtics 0.2

set ylabel "Harmonic Current"
set xlabel "Bias Voltage"

plot "Ideal-30_LO_0-2_Z_1-0_V_0-85_IF_0-01_Z_1-0_2harm.dat" using 1:(sqrt(2)*$9) title "Fundamental" with lines

replot "Ideal-30_LO_0-2_Z_1-0_V_0-85_IF_0-01_Z_1-0_2harm.dat" using 1:(sqrt(2)*$10) title "1st Harmonic" with lines

set terminal postscript eps "Helvetica" 18
set output "Ideal-30_LO_0-2_Z_1-0_V_0-85_IF_0-01_Z_1-0_2harm_currents.eps"

replot

set output "/dev/null"