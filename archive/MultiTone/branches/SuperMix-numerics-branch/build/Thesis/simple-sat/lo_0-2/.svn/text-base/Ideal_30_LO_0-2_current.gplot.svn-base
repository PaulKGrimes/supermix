set size 0.9,1.0

set xlabel "Bias Voltage"
set ylabel "DC Current"

set key left

set xrange [0:2]
set yrange [0:2]

plot "Ideal_30_LO_0-2_V_0-5_Bias_0-9_USB_mixer.dat" using 1:2 title "Unpumped" with lines

replot "Ideal_30_LO_0-2_V_0-5_Bias_0-9_USB_mixer.dat" using 1:4 title "V_{LO} = 0.5" with lines

replot "Ideal_30_LO_0-2_V_0-7_Bias_0-9_USB_mixer.dat" using 1:4 title "V_{LO} = 0.7" with lines

replot "Ideal_30_LO_0-2_V_0-9_Bias_0-9_USB_mixer.dat" using 1:4 title "V_{LO} = 0.9" with lines

replot "Ideal_30_LO_0-2_V_1-1_Bias_0-9_USB_mixer.dat" using 1:4 title "V_{LO} = 1.1" with lines

set terminal postscript eps enhanced "Helvetica" 16

set output "Ideal_30_LO_0-2_current.eps"

replot

set output "/dev/null"


